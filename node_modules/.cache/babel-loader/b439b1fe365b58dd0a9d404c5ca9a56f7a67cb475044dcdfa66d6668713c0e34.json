7\n      }, this)]\n    }, void 0, true, {\n      fileName: _jsxFileName,\n      lineNumber: 32,\n      columnNumber: 5\n    }, this)\n  }, void 0, false, {\n    fileName: _jsxFileName,\n    lineNumber: 31,\n    columnNumber: 5\n  }, this);\n}\n_c = UserAgreement;\nvar _c;\n$RefreshReg$(_c, \"UserAgreement\");","map":{"version":3,"names":["React","Link","jsxDEV","_jsxDEV","styles","root","minHeight","overflowY","overflowX","WebkitOverflowScrolling","background","wrap","padding","maxWidth","margin","lineHeight","fontFamily","color","h1","fontSize","fontWeight","marginBottom","h2","marginTop","p","link","textDecoration","linkHover","back","display","UserAgreement","style","children","fileName","_jsxFileName","lineNumber","columnNumber","to","_c","$RefreshReg$"],"sources":["C:/Users/Vladislav/Documents/GitHub/aist-messenger/src/pages/UserAgreement.jsx"],"sourcesContent":["import React from 'react';\r\nimport { Link } from 'react-router-dom';\r\n\r\nconst styles = {\r\n  root: {\r\n    minHeight: '100vh',\r\n    overflowY: 'auto',\r\n    overflowX: 'hidden',\r\n    WebkitOverflowScrolling: 'touch',\r\n    background: '#f5f5f7',\r\n  },\r\n  wrap: {\r\n    padding: '24px 20px 48px',\r\n    maxWidth: 720,\r\n    margin: '0 auto',\r\n    lineHeight: 1.6,\r\n    fontFamily: '-apple-system, BlinkMacSystemFont, \"Segoe UI\", Roboto, sans-serif',\r\n    color: '#1d1d1f',\r\n    background: '#f5f5f7',\r\n  },\r\n  h1: { fontSize: 28, fontWeight: 700, marginBottom: 24, color: '#1d1d1f' },\r\n  h2: { fontSize: 18, fontWeight: 600, marginTop: 28, marginBottom: 10, color: '#1d1d1f' },\r\n  p: { margin: '0 0 12px', fontSize: 15, color: '#1d1d1f' },\r\n  link: { color: '#0066cc', textDecoration: 'none' },\r\n  linkHover: { textDecoration: 'underline' },\r\n  back: { display: 'inline-block', marginTop: 32, fontSize: 15, color: '#0066cc' },\r\n};\r\n\r\nexport default function UserAgreement() {\r\n  return (\r\n    <div style={styles.root}>\r\n    <div style={styles.wrap}>\r\n      <h1 style={styles.h1}>Пользовательское соглашение</h1>\r\n      <p style={styles.p}>\r\n        Настоящее Пользовательское соглашение (далее — Соглашение) регулирует отношения между оператором сервиса AIST (далее — Сервис, Оператор) и пользователем (далее — Пользователь) в связи с использованием мессенджера и сопутствующих услуг.\r\n      </p>\r\n      <p style={styles.p}>\r\n        Регистрируясь или используя Сервис, Пользователь подтверждает, что ознакомился с Соглашением, понимает его условия и принимает их в полном объёме.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>1. Общие положения</h2>\r\n      <p style={styles.p}>\r\n        1.1. Сервис предоставляет возможность обмена сообщениями, голосовой и видеосвязи, создания каналов и групп. Использование Сервиса осуществляется в соответствии с законодательством Российской Федерации.\r\n      </p>\r\n      <p style={styles.p}>\r\n        1.2. К отношениям сторон применяются положения Гражданского кодекса РФ, Федерального закона от 27.07.2006 № 152-ФЗ «О персональных данных», а также иные применимые нормы права РФ.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>2. Регистрация и учётная запись</h2>\r\n      <p style={styles.p}>\r\n        2.1. Для использования Сервиса Пользователь указывает номер мобильного телефона и подтверждает его путём ввода кода, направленного Оператором (через Telegram-бота, SMS или иным способом). Регистрация означает заключение Соглашения.\r\n      </p>\r\n      <p style={styles.p}>\r\n        2.2. Пользователь обязан хранить конфиденциальность данных учётной записи и не передавать их третьим лицам.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>3. Обработка персональных данных</h2>\r\n      <p style={styles.p}>\r\n        3.1. В соответствии с Федеральным законом № 152-ФЗ «О персональных данных» Оператор обрабатывает персональные данные Пользователя в целях предоставления Сервиса, в том числе: номер телефона, отображаемое имя, никнейм (логин), данные профиля (фото, статус), содержание переписки и метаданные сообщений, данные об устройствах и сессиях.\r\n      </p>\r\n      <p style={styles.p}>\r\n        3.2. Обработка осуществляется на основании согласия Пользователя (при регистрации и принятии Соглашения), а также в случаях, предусмотренных законом. Согласие может быть отозвано путём удаления учётной записи и обращения к Оператору.\r\n      </p>\r\n      <p style={styles.p}>\r\n        3.3. Оператор вправе хранить и обрабатывать персональные данные на серверах, расположенных на территории Российской Федерации, в объёме, необходимом для работы Сервиса и исполнения законодательства.\r\n      </p>\r\n      <p style={styles.p}>\r\n        3.4. Передача персональных данных третьим лицам допускается только в случаях, предусмотренных законом, или с согласия Пользователя. Передача данных за пределы РФ осуществляется в соответствии с требованиями законодательства РФ.\r\n      </p>\r\n      <p style={styles.p}>\r\n        3.5. Пользователь имеет право на доступ к своим персональным данным, их уточнение, блокирование или уничтожение в порядке, установленном законом. Запросы направляются Оператору способами, указанными в разделе «Контакты» Сервиса.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>4. Использование Сервиса</h2>\r\n      <p style={styles.p}>\r\n        4.1. Пользователь обязуется использовать Сервис добросовестно, не нарушать законодательство РФ и не распространять незаконный контент, не призывать к насилию, не посягать на права третьих лиц, не размещать спам и вредоносное ПО.\r\n      </p>\r\n      <p style={styles.p}>\r\n        4.2. Запрещается использование Сервиса для обхода блокировок, навязанных законодательством, а также для деятельности, противоречащей публичному порядку и законодательству РФ.\r\n      </p>\r\n      <p style={styles.p}>\r\n        4.3. Оператор вправе ограничить или прекратить доступ Пользователя к Сервису при нарушении Соглашения или требований закона, с сохранением возможности удаления данных в соответствии с законом.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>5. Интеллектуальная собственность и контент</h2>\r\n      <p style={styles.p}>\r\n        5.1. Пользователь сохраняет права на контент, который он размещает. Размещая контент, Пользователь предоставляет Оператору неисключительную лицензию на использование, хранение и передачу такого контента в объёме, необходимом для работы Сервиса.\r\n      </p>\r\n      <p style={styles.p}>\r\n        5.2. Сервис, его интерфейс, логотипы и технологии принадлежат Оператору или правообладателям. Использование элементов Сервиса без разрешения не допускается.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>6. Ограничение ответственности</h2>\r\n      <p style={styles.p}>\r\n        6.1. Сервис предоставляется «как есть». Оператор не гарантирует бесперебойную работу при форс-мажоре, технических работах или действиях третьих лиц. Ответственность Оператора ограничена в пределах, допускаемых законодательством РФ.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>7. Изменение Соглашения</h2>\r\n      <p style={styles.p}>\r\n        7.1. Оператор вправе изменять Соглашение. Актуальная редакция размещается в Сервисе. Продолжение использования Сервиса после публикации изменений означает принятие новой редакции. При существенных изменениях Пользователь может быть уведомлён через Сервис.\r\n      </p>\r\n\r\n      <h2 style={styles.h2}>8. Заключительные положения</h2>\r\n      <p style={styles.p}>\r\n        8.1. По вопросам, не урегулированным Соглашением, применяются нормы законодательства Российской Федерации.\r\n      </p>\r\n      <p style={styles.p}>\r\n        8.2. По вопросам обработки персональных данных и использования Сервиса Пользователь может обратиться к Оператору способами, указанными на сайте или в приложении Сервиса.\r\n      </p>\r\n\r\n      <p style={styles.back}>\r\n        <Link to=\"/login\" style={styles.link}>← Вернуться к входу</Link>\r\n      </p>\r\n    </div>\r\n    </div>\r\n  );\r\n}\r\n"],"mappings":";AAAA,OAAO